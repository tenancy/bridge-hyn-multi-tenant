{
    "name": "tenancy/multi-tenant-bridge",
    "description": "Provides a bridge to port functionality of hyn/multi-tenant to tenancy/tenancy",
    "keywords": ["database", "tenancy", "hyn"],
    "license": "MIT",
    "require": {
        "tenancy/tenancy": "*",
        "hyn/multi-tenant": ">= 5.3"
    },
    "require-dev": {
        "laravel/laravel": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Tenancy\\Bridge\\Hyn\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "DaniÃ«l Klabbers",
            "email": "daniel@klabbers.email",
            "homepage": "http://luceos.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Tenancy\\Bridge\\Hyn\\HynServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
